{
    "include": [
        "src"
    ],
    "compilerOptions": {
        "outDir": "dist",
        "target": "ESNEXT",
        "module": "ESNext",
        "moduleResolution": "Node",
        "experimentalDecorators":true,
        "jsx": "react-jsx",
        "emitDecoratorMetadata": true,
        "strict": true,
        "declaration": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "esModuleInterop": true
    }
}